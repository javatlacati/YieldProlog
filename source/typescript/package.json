{
  "name": "tyieldprolog",
  "version": "1.0.0",
  "description": "A Prolog implementation based on generators",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "dist/YP.d.ts",
  "type": "module",
  "files": [
    "/dist",
    "lib/"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "compile": "tsc",
    "watch:build": "tsc --watch",
    "build": "tsc  --build",
    "clean": "tsc --build --clean",
    "build_webpack": "webpack",
    "webpack_build": "webpack-cli --config webpack.config.cjs",
    "tsc_export": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublishOnly": "npm run tsc",
    "test": "ts-mocha -p tsconfig.json test/**/*.spec.ts",
    "test1": "mocha --loader=ts-node/esm test/**/*.spec.ts",
    "coverage": "nyc --loader ts-node/esm npm run test"
  },
  "keywords": [
    "prolog",
    "functional",
    "programming"
  ],
  "author": "Ruslan LÃ³pez",
  "license": "0BSD",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.17",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.7",
    "assert": "^2.1.0",
    "chai": "^4.5.0",
    "esm": "^3.2.25",
    "mocha": "^10.7.0",
    "nyc": "^17.0.0",
    "ts-loader": "^9.5.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude-after-remap": false,
    "check-coverage": true,
    "all": true,
    "cache": false,
    "source-map": true,
    "produce-source-map": true,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "**/*.spec.ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "node-option": [
      "no-warnings",
      "experimental-specifier-resolution=node",
      "loader=ts-node/esm"
    ]
  }
}
